pipeline {
    agent any

    stages {
        stage('Clean') {
            steps {
                echo 'cleaning...'
            }
        }
        stage('Build') {
            steps {
                echo 'building...'
            }
        }
        stage('Test'){
            steps {
                echo 'testing...'
            }
        }
        stage('Package') {
            steps {
                echo 'packaging...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying...'
            }
        }
    }
}